        {% if app.user  %}
        <div class="flex items-center px-5">
          <div class="ml-3">
            <div class="text-sm font-medium leading-none text-gray-400">Hola {{ app.user.email }}</div>
          </div>
        </div>
        {% endif %}
        <div class="mt-3 space-y-1 px-2">
          {% if app.user  %}
            <a href="{{ path('front.v1.all.subdomain') }}" class="block rounded-md px-3 py-2 text-base font-medium text-gray-400 hover:bg-gray-700 hover:text-white" aria-current="page">Panel</a>
            <a href="/cuenta" class="block rounded-md px-3 py-2 text-base font-medium text-gray-400 hover:bg-gray-700 hover:text-white">Cuenta</a>
            <a href="{{ path('front.v1.user.logout') }}" class="block rounded-md px-3 py-2 text-base font-medium text-gray-400 hover:bg-gray-700 hover:text-white">Cerrar sesi贸n</a>
          {% else %}
            <a href="/registro" class="block rounded-md px-3 py-2 text-base font-medium text-gray-400 hover:bg-gray-700 hover:text-white" aria-current="page">Registro</a>
            <a href="/login" class="block rounded-md px-3 py-2 text-base font-medium text-gray-400 hover:bg-gray-700 hover:text-white" aria-current="page">Iniciar Sesi贸n</a>
