              <a href="/cuenta" class="{{ app.request.attributes.get("_route") in ['cuenta-usuario', 'panel-confirm-delete-account-user'] ? 'bg-gray-700': ''}} text-gray-300 hover:bg-gray-700 hover:text-white rounded-md px-3 py-2 text-sm font-medium">Cuenta</a>
              <a href="{{ path('front.v1.user.logout') }}" class="text-gray-300 hover:bg-gray-700 hover:text-white rounded-md px-3 py-2 text-sm font-medium">Cerrar sesi贸n</a>
