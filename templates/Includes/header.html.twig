                <a href="{{ path('front.v1.add.record.to.subdomain', {'idSubdominio' :app.request.attributes.get('idSubdominio') }) }}" class="ms-1 text-sm font-medium text-black-400 hover:text-black-600 md:ms-2 dark:text-black-400">DNS</a>
              </div>
